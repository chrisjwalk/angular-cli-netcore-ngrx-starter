{
  "name": "nx-ng-net-core",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "nx": "nx",
    "ngcc": "ngcc",
    "start": "nx run-many --target=serve --projects=web-app,api --parallel",
    "build": "nx run-many --target=build --projects=web-app,api --parallel",
    "build:prod": "npm run build -- --configuration production",
    "build:api": "nx build api",
    "build:api:prod": "nx build api --configuration production",
    "build:web-app": "nx build web-app",
    "build:web-app:prod": "nx build web-app --configuration production",
    "build:dotnet-builder": "rimraf ./tools/builders/dotnet-builder/dist && tsc -p ./tools/builders/dotnet-builder",
    "clean": "rimraf ./dist ./junit ./coverage ./.angular",
    "serve:api": "nx serve api",
    "test": "nx affected --target=test",
    "test:ci": "npm run test -- --configuration ci",
    "test:all": "nx run-many --target=test --all --configuration ci",
    "lint": "nx affected --target=lint --fix",
    "lint:all": "nx run-many --target=lint --all",
    "lint:workspace": "nx workspace-lint && ng lint",
    "e2e": "nx e2e web-app-e2e",
    "e2e:ci": "npm run e2e -- --configuration production",
    "e2e:watch": "npm run e2e -- --configuration watch",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "npm run lint:fix && npm run format:write && npm run beautify",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "beautify": "js-beautify --type=html -r -f ./**/*.html",
    "update": "nx migrate latest",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "dotnet:restore": "dotnet restore apps/api",
    "dotnet:coverage": "dotnet tool install --global dotnet-coverage",
    "dotnet:clean": "dotnet clean apps/api",
    "dotnet:build": "dotnet build apps/api",
    "preinstall": "npm run dotnet:restore",
    "postinstall": "npm run build:dotnet-builder",
    "prepare": "husky install",
    "migrate:run": "npm i && npx nx migrate --run-migrations",
    "migrate:angular:core": "npx nx migrate @angular/core@latest",
    "migrate:angular:cli": "npx nx migrate @angular/cli@latest",
    "migrate:angular:material": "npx nx migrate @angular/material@latest",
    "migrate:nx": "npx nx migrate latest",
    "migrate:all": "npm run migrate:nx && npm run migrate:run && npm run migrate:angular:core && npm run migrate:run && npm run migrate:angular:cli && npm run migrate:run && npm run migrate:angular:material && npm run migrate:run"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "16.1.3",
    "@angular/cdk": "16.1.3",
    "@angular/common": "16.1.3",
    "@angular/compiler": "16.1.3",
    "@angular/core": "16.1.3",
    "@angular/forms": "16.1.3",
    "@angular/material": "16.1.3",
    "@angular/platform-browser": "16.1.3",
    "@angular/platform-browser-dynamic": "16.1.3",
    "@angular/router": "16.1.3",
    "@angular/service-worker": "16.1.3",
    "@ngrx/component-store": "^16.0.1",
    "@nx/angular": "16.5.0",
    "marked": "^4.2.12",
    "moment": "^2.29.4",
    "ngx-markdown": "^16.0.0",
    "rxjs": "~7.8.1",
    "tslib": "^2.5.3",
    "zone.js": "0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "16.1.3",
    "@angular-devkit/core": "16.1.3",
    "@angular-devkit/schematics": "16.1.3",
    "@angular-eslint/eslint-plugin": "16.0.3",
    "@angular-eslint/eslint-plugin-template": "16.0.3",
    "@angular-eslint/template-parser": "16.0.3",
    "@angular/cli": "16.1.3",
    "@angular/compiler-cli": "16.1.3",
    "@angular/language-service": "16.1.3",
    "@nx/cypress": "16.5.0",
    "@nx/eslint-plugin": "16.5.0",
    "@nx/jest": "16.5.0",
    "@nx/js": "16.5.0",
    "@nx/linter": "16.5.0",
    "@nx/node": "16.5.0",
    "@nx/web": "16.5.0",
    "@nx/workspace": "16.5.0",
    "@schematics/angular": "16.1.3",
    "@testing-library/angular": "^14.1.1",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/jest": "^29.5.2",
    "@types/marked": "^4.0.8",
    "@types/node": "^18.16.19",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "@typescript-eslint/types": "^5.61.0",
    "autoprefixer": "^10.4.0",
    "cypress": "^12.15.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-cypress": "^2.13.3",
    "husky": "^8.0.3",
    "jest": "^29.6.0",
    "jest-environment-jsdom": "^29.6.0",
    "jest-junit": "^16.0.0",
    "jest-preset-angular": "^13.1.1",
    "js-beautify": "^1.14.8",
    "lint-staged": "^13.2.2",
    "nx": "16.5.0",
    "postcss": "^8.4.5",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "tailwindcss": "^3.0.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "5.1.3"
  },
  "lint-staged": {
    "*.{ts,js}": "eslint --cache --cache-location=.husky/_ --fix",
    "*.{ts,js,css,scss,md,mdx}": "prettier --write",
    "*.html": "js-beautify --type=html -r"
  }
}
