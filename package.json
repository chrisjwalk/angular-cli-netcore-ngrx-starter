    "esbuild": "0.19.12",
    "eslint": "9.20.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-playwright": "2.2.0",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-environment-node": "^29.4.1",
    "jest-junit": "16.0.0",
    "jest-preset-angular": "14.5.1",
    "jsdom": "26.0.0",
    "jsonc-eslint-parser": "2.4.0",
    "lint-staged": "15.4.3",
    "nx": "20.4.4",
    "postcss": "8.5.2",
    "prettier": "3.5.1",
    "rimraf": "6.0.1",
    "tailwindcss": "3.4.17",
    "ts-jest": "29.2.5",
