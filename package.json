    "@angular/router": "19.1.6",
    "@angular/service-worker": "19.1.6",
    "@ngrx/operators": "19.0.1",
    "@ngrx/signals": "19.0.1",
    "lodash": "4.17.21",
    "marked": "15.0.7",
    "moment": "2.30.1",
    "ngx-markdown": "19.1.0",
    "rxjs": "7.8.1",
    "tslib": "2.8.1"
  },
