    "@vitest/coverage-v8": "3.1.2",
    "@vitest/ui": "3.1.2",
    "angular-eslint": "19.3.0",
    "autoprefixer": "10.4.21",
    "commander": "13.1.0",
    "dotenv": "16.5.0",
    "esbuild": "0.25.3",
    "eslint": "9.25.1",
    "eslint-config-prettier": "10.1.2",
    "eslint-plugin-playwright": "2.2.0",
    "husky": "9.1.7",
    "jiti": "2.4.2",
    "jsdom": "26.1.0",
    "jsonc-eslint-parser": "2.4.0",
    "lint-staged": "15.5.1",
    "nx": "20.8.1",
    "postcss": "8.5.3",
    "prettier": "3.5.3",
    "rimraf": "6.0.1",
    "tailwindcss": "4.1.4",
    "ts-node": "10.9.2",
    "typescript": "5.7.3",
    "typescript-eslint": "8.31.0",
    "vite": "6.3.3",
    "vitest": "3.1.2",
    "zone.js": "^0.15.0"
  },
  "lint-staged": {
    "*.{ts,js}": "eslint --cache --cache-location=.husky/_ --fix",
