    "@tailwindcss/postcss": "4.1.8",
    "lodash": "4.17.21",
    "marked": "15.0.12",
    "moment": "2.30.1",
    "ngx-markdown": "20.0.0",
    "rxjs": "7.8.2",
    "tslib": "2.8.1"
  },
