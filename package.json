{
  "name": "nx-ng-net-core",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "nx": "nx",
    "ngcc": "ngcc",
    "start": "nx run-many --target=serve --projects=web-app,api --parallel",
    "build": "nx run-many --target=build --projects=web-app,api --parallel",
    "build:prod": "npm run build -- --configuration production",
    "build:api": "nx build api",
    "build:api:prod": "nx build api --configuration production",
    "build:web-app": "nx build web-app",
    "build:web-app:prod": "nx build web-app --configuration production",
    "build:dotnet-builder": "rimraf ./tools/builders/dotnet-builder/dist && tsc -p ./tools/builders/dotnet-builder",
    "clean": "rimraf ./dist ./junit ./coverage ./.angular ./.nx && nx reset",
    "clean:node_modules": "rimraf ./node_modules ./package-lock.json",
    "serve:api": "nx serve api",
    "serve:prod": "nx serve-static web-app",
    "test": "nx affected --target=test",
    "test:ci": "npm run test -- --configuration ci",
    "test:all": "nx run-many --target=test --all --configuration ci",
    "lint": "nx affected --target=lint --fix",
    "lint:all": "nx run-many --target=lint --all",
    "lint:workspace": "nx workspace-lint && ng lint",
    "e2e": "nx e2e web-app-e2e",
    "e2e:ci": "npm run e2e -- --configuration production",
    "e2e:watch": "npm run e2e -- --configuration watch",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "npm run lint && npm run format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "dotnet:restore": "dotnet restore apps/api",
    "dotnet:coverage": "dotnet tool install --global dotnet-coverage",
    "dotnet:clean": "dotnet clean apps/api",
    "dotnet:build": "dotnet build apps/api",
    "preinstall": "npm run dotnet:restore",
    "postinstall": "npm run build:dotnet-builder",
    "prepare": "husky install",
    "migrate:run": "npm i && npx nx migrate --run-migrations",
    "migrate:angular:core": "npx nx migrate @angular/core@latest",
    "migrate:angular:cli": "npx nx migrate @angular/cli@latest",
    "migrate:angular:material": "npx nx migrate @angular/material@latest",
    "migrate:angular:schematics": "npx nx migrate @schematics/angular@latest",
    "migrate:ngrx": "npx nx migrate @ngrx/signals@latest",
    "migrate:nx": "npx nx migrate latest",
    "migrate:all": "npm run migrate:nx && npm run migrate:angular:core && npm run migrate:angular:cli && npm run migrate:angular:schematics && npm run migrate:angular:material && npm run clean:node_modules && npm run migrate:run",
    "update-packages": "nx build update-packages && node ./dist/tools/update-packages/main.js typescript marked"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "17.1.0",
    "@angular/cdk": "17.1.0",
    "@angular/common": "17.1.0",
    "@angular/compiler": "17.1.0",
    "@angular/core": "17.1.0",
    "@angular/forms": "17.1.0",
    "@angular/material": "17.1.0",
    "@angular/platform-browser": "17.1.0",
    "@angular/platform-browser-dynamic": "17.1.0",
    "@angular/router": "17.1.0",
    "@angular/service-worker": "17.1.0",
    "@ngrx/operators": "17.1.0",
    "@ngrx/signals": "17.1.0",
    "@nx/angular": "17.2.8",
    "marked": "^9.1.6",
    "moment": "2.30.1",
    "ngx-markdown": "17.1.1",
    "rxjs": "7.8.1",
    "tslib": "2.6.2",
    "zone.js": "0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "17.0.10",
    "@angular-devkit/core": "17.0.10",
    "@angular-devkit/schematics": "17.0.10",
    "@angular-eslint/eslint-plugin": "17.2.0",
    "@angular-eslint/eslint-plugin-template": "17.2.0",
    "@angular-eslint/template-parser": "17.2.0",
    "@angular/cli": "17.0.10",
    "@angular/compiler-cli": "17.1.0",
    "@angular/language-service": "17.1.0",
    "@nx/cypress": "17.2.8",
    "@nx/esbuild": "17.2.8",
    "@nx/eslint": "17.2.8",
    "@nx/eslint-plugin": "17.2.8",
    "@nx/jest": "17.2.8",
    "@nx/js": "17.2.8",
    "@nx/node": "17.2.8",
    "@nx/web": "17.2.8",
    "@nx/workspace": "17.2.8",
    "@schematics/angular": "17.0.10",
    "@swc-node/register": "~1.6.7",
    "@swc/core": "1.3.104",
    "@testing-library/angular": "15.1.0",
    "@testing-library/jest-dom": "6.2.0",
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "@types/jest": "29.5.11",
    "@types/node": "20.11.5",
    "@typescript-eslint/eslint-plugin": "6.19.0",
    "@typescript-eslint/parser": "6.19.0",
    "@typescript-eslint/types": "6.19.0",
    "autoprefixer": "10.4.17",
    "cypress": "13.6.3",
    "dotenv": "16.3.1",
    "esbuild": "0.19.11",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-cypress": "2.15.1",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-environment-node": "^29.4.1",
    "jest-junit": "16.0.0",
    "jest-preset-angular": "14.0.0",
    "jsonc-eslint-parser": "^2.1.0",
    "lint-staged": "15.2.0",
    "nx": "17.2.8",
    "postcss": "8.4.33",
    "prettier": "3.2.4",
    "rimraf": "5.0.5",
    "tailwindcss": "3.4.1",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.2",
    "typescript": "5.2.2"
  },
  "lint-staged": {
    "*.{ts,js}": "eslint --cache --cache-location=.husky/_ --fix",
    "*.{ts,js,css,scss,md,mdx}": "prettier --write"
  }
}

