    "@vitest/coverage-v8": "3.2.2",
    "@vitest/ui": "3.2.2",
    "angular-eslint": "20.0.0",
    "autoprefixer": "10.4.21",
    "commander": "14.0.0",
    "dotenv": "16.5.0",
    "esbuild": "0.25.5",
    "eslint": "9.28.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-playwright": "2.2.0",
    "husky": "9.1.7",
    "jiti": "2.4.2",
    "jsdom": "26.1.0",
    "jsonc-eslint-parser": "2.4.0",
    "lint-staged": "16.1.0",
    "nx": "21.1.3",
    "postcss": "^8.5.3",
    "prettier": "3.5.3",
    "rimraf": "6.0.1",
    "tailwindcss": "4.1.8",
    "ts-node": "10.9.2",
    "typescript": "5.8.3",
    "typescript-eslint": "8.33.1",
    "vite": "6.3.5",
    "vitest": "3.2.2"
  },
  "lint-staged": {
    "*.{ts,js}": "eslint --cache --cache-location=.husky/_ --fix",
