    "@ngrx/signals": "19.1.0",
    "@tailwindcss/postcss": "4.1.4",
    "lodash": "4.17.21",
    "marked": "15.0.11",
    "moment": "2.30.1",
    "ngx-markdown": "19.1.1",
    "rxjs": "7.8.2",
    "tslib": "2.8.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "19.2.9",
