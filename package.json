    "@eslint/eslintrc": "3.2.0",
    "@eslint/js": "9.20.0",
    "@nx/angular": "20.4.4",
    "@nx/devkit": "20.4.4",
    "@nx/eslint": "20.4.4",
    "@nx/eslint-plugin": "20.4.4",
    "@nx/jest": "20.4.4",
