    "@angular-eslint/template-parser": "19.3.0",
    "@angular/cli": "19.2.9",
    "@angular/compiler-cli": "19.2.8",
    "@angular/language-service": "19.2.8",
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.25.1",
    "@nx/angular": "20.8.1",
    "@nx/devkit": "20.8.1",
    "@nx/eslint": "20.8.1",
    "@nx/eslint-plugin": "20.8.1",
    "@nx/js": "20.8.1",
    "@nx/node": "20.8.1",
    "@nx/playwright": "20.8.1",
    "@nx/vite": "20.8.1",
    "@nx/web": "20.8.1",
    "@nx/workspace": "20.8.1",
    "@playwright/test": "1.52.0",
    "@schematics/angular": "19.2.9",
    "@swc-node/register": "1.10.10",
    "@swc/core": "1.11.22",
    "@testing-library/angular": "17.3.7",
    "@testing-library/dom": "10.4.0",
